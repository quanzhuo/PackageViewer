<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.github.quanzhuo.pkgviewer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PackageListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".PackageOverviewActivity"
            android:label="@string/pkg_overview">
        </activity>
        <activity android:name=".AllActivities">
        </activity>
        <activity android:name=".AllServices">
        </activity>
        <activity android:name=".AllReceivers">
        </activity>
        <activity android:name=".AllProviders">
        </activity>
    </application>

</manifest>